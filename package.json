{
  "name": "godested-site",
  "description": "Godested site",
  "version": "1.0.0",
  "private": true,
  "author": "Zaitsev Ilya <godested@gmail.com>",
  "license": "MIT",
  "scripts": {
    "prestart": "npm run tsm:build",
    "start": "gatsby develop",
    "prebuild": "npm run tsm:build && npm run type-check",
    "deploy": "gh-pages -d public",
    "build": "gatsby build",
    "build:prefix-paths": "gatsby build --prefix-paths",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "tsm:watch": "npm run tsm:build -- --watch",
    "tsm:build": "node ./node_modules/.bin/typed-scss-modules \"./src/**/*.module.scss\" --aliasPrefixes.\"~\" \"src/\" --logLevel \"error\"",
    "type-check": "node ./node_modules/.bin/tsc --noEmit -p ./tsconfig.json",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|tsx)\" --write",
    "postinstall": "husky install",
    "test": "jest",
    "commit": "cz"
  },
  "lint-staged": {
    "./src/**/*.{ts,tsx}": [
      "npm run lint --fix",
      "npm run format"
    ]
  },
  "dependencies": {
    "classnames": "2.3.1",
    "gatsby": "4.11.1",
    "normalize.css": "8.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet": "6.1.0",
    "rxjs": "7.5.5",
    "simplex-noise": "3.0.1"
  },
  "devDependencies": {
    "@babel/preset-react": "7.16.7",
    "@babel/preset-typescript": "7.16.7",
    "@commitlint/cli": "16.2.3",
    "@commitlint/config-conventional": "16.2.1",
    "@testing-library/dom": "8.12.0",
    "@testing-library/jest-dom": "5.16.3",
    "@testing-library/react": "13.0.0",
    "@testing-library/react-hooks": "7.0.2",
    "@types/jest": "27.4.1",
    "@types/node": "16.11.26",
    "@types/react": "17.0.43",
    "@types/react-dom": "17.0.14",
    "@types/react-helmet": "6.1.5",
    "@typescript-eslint/eslint-plugin": "5.18.0",
    "@typescript-eslint/parser": "5.18.0",
    "babel-jest": "27.5.1",
    "babel-preset-gatsby": "2.11.1",
    "commitizen": "4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.12.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "2.7.1",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jest": "26.1.3",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.29.4",
    "eslint-plugin-react-hooks": "4.4.0",
    "gatsby-plugin-preact": "6.12.1",
    "gatsby-plugin-provide-react": "1.0.2",
    "gatsby-plugin-react-helmet": "5.11.0",
    "gatsby-plugin-react-svg": "3.1.0",
    "gatsby-plugin-sass": "5.11.1",
    "gh-pages": "3.2.3",
    "husky": "7.0.4",
    "identity-obj-proxy": "3.0.0",
    "jest": "27.5.1",
    "lint-staged": "12.3.7",
    "preact": "10.7.1",
    "preact-render-to-string": "5.1.21",
    "prettier": "2.6.2",
    "sass": "1.50.0",
    "ts-jest": "27.1.4",
    "tsconfig-paths-webpack-plugin": "3.5.2",
    "typed-scss-modules": "6.2.0",
    "typescript": "4.4.2"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
